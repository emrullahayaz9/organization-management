<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
    <div class="container">
        <h3>Organizer Interface</h3>
        <div class="col-6">
            <h4>Active Events</h4>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Event Date</th>
                        <th scope="col">Customer Name</th>
                        <th scope="col">Customer Phone Number</th>
                        <th scope="col">Venue</th>
                        <th scope="col">Location Address</th>
                        <th scope="col">Customer Email</th>
                    </tr>
                </thead>
                <tbody>
                    <% events.forEach(function(event) { %>
                        <tr>
                            <td><%= event.event_date.toDateString() %></td>
                            <td><%= event.customer_first_name %> <%= event.customer_last_name %></td>
                            <td><%= event.customer_phone_number %></td>
                            <td><%= event.venue_name %></td>
                            <td> <%= event.venue_city %>, <%= event.venue_state %>, <%= event.venue_street %>, <%= event.venue_door_number %>   </td>
                            <td> <%= event.customer_email %>   </td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</body>
</html>